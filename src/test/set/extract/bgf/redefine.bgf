<bgf:grammar xmlns:bgf="http://planet-sl.org/bgf">
  <bgf:production>
    <nonterminal>a</nonterminal>
    <bgf:expression>
      <sequence>
        <bgf:expression>
          <terminal>{</terminal>
        </bgf:expression>
        <bgf:expression>
          <nonterminal>b</nonterminal>
        </bgf:expression>
        <bgf:expression>
          <terminal>}</terminal>
        </bgf:expression>
      </sequence>
    </bgf:expression>
  </bgf:production>
  <bgf:production>
    <nonterminal>b</nonterminal>
    <bgf:expression>
      <nonterminal>a</nonterminal>
    </bgf:expression>
  </bgf:production>
</bgf:grammar>
