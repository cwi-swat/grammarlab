<bgf:grammar xmlns:bgf="http://planet-sl.org/bgf">
  <bgf:production>
    <nonterminal>decs</nonterminal>
    <bgf:expression>
      <nonterminal>dec</nonterminal>
    </bgf:expression>
  </bgf:production>
  <bgf:production>
    <nonterminal>decs</nonterminal>
    <bgf:expression>
      <sequence>
        <bgf:expression><nonterminal>dec</nonterminal></bgf:expression>
        <bgf:expression><nonterminal>decs</nonterminal></bgf:expression>
      </sequence>
    </bgf:expression>
  </bgf:production>
</bgf:grammar>
