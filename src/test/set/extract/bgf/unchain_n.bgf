<?xml version="1.0" encoding="UTF-8"?>
<bgf:grammar xmlns:bgf="http://planet-sl.org/bgf">
	<bgf:production>
		<nonterminal>ClassDeclaration</nonterminal>
		<bgf:expression>
			<nonterminal>NormalClassDeclaration</nonterminal>
		</bgf:expression>
	</bgf:production>
	<bgf:production>
		<nonterminal>NormalClassDeclaration</nonterminal>
		<bgf:expression>
			<sequence>
				<bgf:expression>
					<terminal>class</terminal>
				</bgf:expression>
				<bgf:expression>
					<nonterminal>Identifier</nonterminal>
				</bgf:expression>
				<bgf:expression>
					<nonterminal>ClassBody</nonterminal>
				</bgf:expression>
			</sequence>
		</bgf:expression>
	</bgf:production>
	<bgf:production>
		<nonterminal>ClassBody</nonterminal>
		<bgf:expression>
			<sequence>
				<bgf:expression>
					<terminal>{</terminal>
				</bgf:expression>
				<bgf:expression>
					<optional>
						<bgf:expression>
							<nonterminal>ClassBodyDeclarations</nonterminal>
						</bgf:expression>
					</optional>
				</bgf:expression>
				<bgf:expression>
					<terminal>}</terminal>
				</bgf:expression>
			</sequence>
		</bgf:expression>
	</bgf:production>
</bgf:grammar>
